{
    "name": "newsman/magento2x",
    "type": "magento2-component",
    "license": "Apache-2.0",
    "version": "2.5.27",
    "authors": [
        {
            "name": "Daniel Ifrim"
        }
    ],
    "description": "Newsman module for Magento. Sync your Magento customers / subscribers to Newsman list / segments.",
    "homepage": "https://github.com/Newsman/Magento2-Newsman",
    "keywords": [
        "magento",
        "magento2",
        "newsletter",
        "search",
        "remarketing",
        "marketing",
        "email",
        "subscriber"
    ],
    "require": {
        "php": ">=7.2",
        "magento/framework": ">100.0",
        "magento/module-backend": ">100.0",
        "magento/module-catalog": ">100.0",
        "magento/module-catalog-search": ">100.0",
        "magento/module-checkout": ">100.0",
        "magento/module-config": ">100.0",
        "magento/module-quote": ">100.0",
        "magento/module-newsletter": ">100.0",
        "magento/module-sales": ">100.0",
        "magento/module-sales-rule": ">100.0",
        "magento/module-store": ">100.0",
        "ext-curl": "*",
        "greenlion/php-sql-parser": "^4.7"
    },
    "replace": {
        "dazoot/module-dazoot-newsman": "self.version",
        "dazoot/module-dazoot-newsman-marketing": "self.version"
    },
    "autoload": {
        "files": [
            "src/module-dazoot-newsman/registration.php",
            "src/module-dazoot-newsman-marketing/registration.php"
        ],
        "psr-4": {
            "Dazoot\\Newsman\\": "src/module-dazoot-newsman",
            "Dazoot\\Newsmanmarketing\\": "src/module-dazoot-newsman-marketing"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
